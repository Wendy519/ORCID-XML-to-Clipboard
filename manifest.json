{
    "name": "ORCID XML to Clipboard",
    "manifest_version": 2,
    "version": "0.1",
    "description": "Run process on page activated by click in extension popup",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Clip ORCID",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://orcid.org/*"
    ]
}